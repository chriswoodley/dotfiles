# Path where Git, node.js, and composer is installed
export PATH="/usr/local/bin:/usr/bin:/usr/local/sbin:/usr/local/git/bin:$PATH"

# Path where MySQL is installed
export PATH="/usr/local/opt/mysql@5.7/bin:$PATH

# Path where composer packages (i.e. laravel) are installed
export PATH="$PATH:$HOME/.composer/vendor/bin"

# Path where java is installed
export JAVA_HOME=$(/usr/libexec/java_home)

export PATH="/usr/local/opt/gpg-agent/bin:$PATH"

export PATH="/usr/local/opt/qt/bin:$PATH"

# Path where global node modules are installed
export PATH=~/.npm-global/bin:$PATH

# Path where NVM is installed
export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion
